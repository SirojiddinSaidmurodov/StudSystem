<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="339.0" prefWidth="180.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.StudentEditorController">
    <VBox layoutX="14.0" layoutY="14.0" spacing="15.0" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="15.0">
        <TextField fx:id="loginField" promptText="Логин"/>
        <PasswordField fx:id="passwordField" promptText="Пароль"/>
        <TextField fx:id="nameField" promptText="Имя"/>
        <TextField fx:id="surnameField" promptText="Фамилия"/>
        <ComboBox fx:id="instituteComboBox" prefWidth="150.0" promptText="Институт"/>
        <ComboBox fx:id="facultyComboBox" prefWidth="150.0" promptText="Факльтет"/>
        <ComboBox fx:id="groupComboBox" prefWidth="150.0" promptText="Группа"/>
    </VBox>
    <HBox layoutX="37.0" layoutY="292.0" spacing="15.0">
        <Button fx:id="ok" mnemonicParsing="false" text="ОК"/>
        <Button fx:id="cancel" mnemonicParsing="false" text="Отмена"/>
    </HBox>
</AnchorPane>
