<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="175.0"
            prefWidth="180.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="app.ResultEditorController">
    <VBox alignment="CENTER" layoutX="14.0" layoutY="14.0" spacing="15.0" AnchorPane.leftAnchor="15.0"
          AnchorPane.topAnchor="15.0">
        <TextField fx:id="markField" promptText="Оценка"/>
        <ComboBox fx:id="examComboBox" prefWidth="150.0" promptText="Сессия"/>
        <ComboBox fx:id="subjectComboBox" prefWidth="150.0" promptText="Предмет"/>
        <HBox alignment="CENTER" spacing="15.0">
            <Button fx:id="ok" mnemonicParsing="false" text="ОК"/>
            <Button fx:id="cancel" mnemonicParsing="false" text="Отмена"/>
        </HBox>
    </VBox>
</AnchorPane>
