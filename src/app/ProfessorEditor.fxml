<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="215.0" prefWidth="180.0"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="app.ProfessorEditorController">
   <VBox layoutX="137.0" layoutY="82.0" spacing="15.0" AnchorPane.bottomAnchor="55.0" AnchorPane.leftAnchor="15.0"
         AnchorPane.rightAnchor="16.0" AnchorPane.topAnchor="15.0">
      <TextField fx:id="nameField" promptText="Имя"/>
      <TextField fx:id="surnameField" promptText="Фамилия"/>
      <TextField fx:id="loginField" promptText="Логин"/>
      <PasswordField fx:id="passwordField" promptText="Пароль"/>
   </VBox>
   <HBox layoutX="37.0" layoutY="175.0" spacing="15.0" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="37.0">
      <Button fx:id="ok" mnemonicParsing="false" text="ОК"/>
      <Button fx:id="cancel" mnemonicParsing="false" text="Отмена"/>
   </HBox>
</AnchorPane>
